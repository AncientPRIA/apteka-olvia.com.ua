{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "cocur/slugify": "^4.0",
        "extended-bread-form-fields": "^0.2.0@beta",
        "fideloper/proxy": "^4.0",
        "hisorange/browser-detect": "3.*",
        "irazasyed/telegram-bot-sdk": "^2.0",
        "jakoch/phantomjs-installer": "2.1.1-p08",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "5.8.*",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "~2.1.0",
        "socialiteproviders/instagram": "^3.0",
        "socialiteproviders/vkontakte": "^4.0",
        "staudenmeir/eloquent-has-many-deep": "1.8",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0",
        "tcg/voyager": "1.3.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-0": {

        },
        "psr-4": {
            "App\\": "app/",
            "Josh\\Component\\PhantomJs\\": "vendor/josh/laravel-phantomjs/src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Models",
            "app/Helpers",
            "app/Processors",
            "app/Facades"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],

        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]

    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
